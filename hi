SELECT DISTINCT WRK_PRCTR_ROLE.PROV_RESOURCE_ID
,WRK_PRCTR_ROLE.PROV_TYPE_PERF_CLM
,WRK_PRCTR_ROLE.ENTITY_TYPE
,WRK_PRCTR_ROLE.DIR_DSPLY_IND
,WRK_PRCTR_ROLE.PROV_PATIENT_AGE_FROM
,WRK_PRCTR_ROLE.PROV_PATIENT_AGE_TO
,WRK_PRCTR_ROLE.PROV_PATIENT_GNDR_RESTRCT
,WRK_PRCTR_ROLE.PROV_PHYSN_STTS_CD
,WRK_PRCTR_ROLE.PRCTR_TRMNTN_RSN_CD
,WRK_PRCTR_ROLE.PROV_PCP_EFCTV_DT
,WRK_PRCTR_ROLE.PROV_PCP_TRMNTN_DT
,WRK_PRCTR_ROLE.PCP_IND
,WRK_PRCTR_ROLE.PROV_CPTN_IND
,WRK_PRCTR_ROLE.PCP_SELECTABILITY_IND
,WRK_PRCTR_ROLE.PRCTR_ROLE_EFCTV_DT
,WRK_PRCTR_ROLE.PRCTR_ROLE_TRMNTN_DT
,WRK_PRCTR_ROLE.PRCTR_ROLE_ID
,WRK_PRCTR_ROLE.LCTN_RESOURCE_ID
,WRK_PRCTR_ROLE.ORG_RESOURCE_ID
,WRK_PRCTR_ROLE.NTWK_RESOURCE_ID
,WRK_PRCTR_ROLE.LCNS_RESOURCE_ID
,WRK_PRCTR_ROLE.PROV_TYPE_PERF_DSPLY_VAL
,WRK_PRCTR_ROLE.PROV_TIER_DESGNTN_IND
FROM PDT.WRK_PRCTR_ROLE
WHERE PRCTR_ROLE_ID = <>;

SELECT DISTINCT PDT.WRK_PRCTR_ROLE.PRCTR_ROLE_ID,PDT.WRK_PRCTR_ROLE.TXNMY_CD,PDT.WRK_PRCTR_ROLE.TXNMY_DSPLY_VAL 
FROM PDT.WRK_PRCTR_ROLE WHERE PRCTR_ROLE_ID = <>; 
SELECT DISTINCT WRK_PRCTR_ROLE.PRCTR_ROLE_ID, WRK_PRCTR_ROLE.PROV_AFFLTN_RESOURCE_ID
FROM PDT.WRK_PRCTR_ROLE WHERE PRCTR_ROLE_ID = <>;

SELECT DISTINCT WRK_PRCTR_ROLE.PRCTR_ROLE_ID, 
--WRK_PRCTR_ROLE.AFFLTN_RESOURCE_ID, 
WRK_PRCTR_ROLE.AFFLTN_TYPE,
CASE WHEN WRK_PRCTR_ROLE.AFFLTN_TYPE = 'PROVGRP' THEN 'Provider Group' 
WHEN WRK_PRCTR_ROLE.AFFLTN_TYPE = 'FACILITY' THEN 'Facility' 
ELSE '' END AS AFFLTN_DSPLY_VAL
FROM PDT.WRK_PRCTR_ROLE
WHERE WRK_PRCTR_ROLE.ORG_RESOURCE_ID IS NOT NULL; 

select distinct vbp.prov_resource_id,vbp.lctn_resource_id,vbp.PROV_MBR_SLF_ATRBTN_IND,vbp.BCBSA_VBP_ID,vbp.prov_enty_id
from pdt.wrk_prctr_role PR
inner join pdt.prov_vbp vbp
on PR.prov_resource_id = vbp.prov_resource_id
AND PR.LCTN_RESOURCE_ID = VBP.LCTN_RESOURCE_ID
where PR.prov_resource_id = <>
order by vbp.prov_resource_id asc, vbp.lctn_resource_id asc;

SELECT DISTINCT PRCTR_ROLE_RESOURCE_ID, PROV_HLTH_SRVC_RESOURCE_ID FROM PDT.WRK_PROV_HLTH_SRVC WHERE PRCTR_ROLE_RESOURCE_ID = <>; 

select distinct pofc.prov_resource_id,pofc.PROV_DAY_OF_WEEK,pofc.PROV_OFC_STRT_TM,pofc.PROV_OFC_END_TM from pdt.wrk_prctr_role PR
inner join pdt.pofc_hours pofc
on PR.prov_resource_id = pofc.prov_resource_id
where pofc.prov_resource_id = <>;
